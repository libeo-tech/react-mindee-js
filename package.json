{
  "name": "react-mindee-js",
  "version": "1.3.0-rc.2",
  "description": "Front-End Computer Vision SDK for React",
  "author": "@mindee",
  "license": "GPL-3.0",
  "repository": "publicMindee/react-mindee-js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "dependencies": {
    "konva": "^8.1.3",
    "pdfjs-dist": "2.6.347",
    "ts-key-enum": "^2.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-patch": "^1.4.3",
    "typescript": "^4.1.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "dev": "tsc --watch",
    "cy:prepare": "yarn build && yarn cy:install",
    "prepare": "ts-patch install -s",
    "build": "tsc",
    "cy:open": "yarn --cwd ./cypress cypress open-ct",
    "cy:run": "yarn --cwd ./cypress cypress run-ct",
    "cy:update": "yarn --cwd ./cypress yarn add ../",
    "cy:install": "yarn --cwd ./cypress install",
    "docs:start": "yarn --cwd ./docs start",
    "docs:install": "yarn --cwd ./docs install",
    "docs:prepare": "yarn build && yarn docs:install",
    "docs:update": "yarn --cwd ./docs add ../",
    "docs:build": "yarn --cwd ./docs build",
    "docs:deploy": "yarn --cwd ./docs deploy"
  },
  "keywords": [
    "react",
    "js",
    "javascript",
    "mindee",
    "SDK",
    "canvas",
    "document",
    "image",
    "segmentation",
    "processing",
    "computer",
    "vision"
  ]
}
